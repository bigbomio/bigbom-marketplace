{
    "name": "test-react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.1.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
        "@fortawesome/free-brands-svg-icons": "^5.1.0",
        "@fortawesome/free-regular-svg-icons": "^5.1.0",
        "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
        "@fortawesome/react-fontawesome": "^0.1.0-11",
        "@material-ui/core": "^1.4.2",
        "axios": "^0.18.0",
        "country-language": "^0.1.7",
        "cross-env": "^5.2.0",
        "history": "^4.7.2",
        "immutable": "^3.8.2",
        "lodash": "^4.17.10",
        "node-sass-chokidar": "^1.3.0",
        "npm-run-all": "^4.1.3",
        "prop-types": "^15.6.1",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-helmet": "^5.2.0",
        "react-intl": "^2.4.0",
        "react-lazyload": "^2.3.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "redux-saga": "^0.16.0",
        "reselect": "^3.0.1",
        "socketcluster-client": "^13.0.0"
    },
    "devDependencies": {
        "react-scripts": "1.1.4",

        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.9.1",
        
        "babel-cli": "^6.26.0",
        "babel-plugin-react-intl": "^2.4.0",
        "babel-preset-stage-0": "^6.24.1",
        "chalk": "^2.4.1",
        "child_process": "^1.0.2",
        "extract-intl": "^2.0.0",
        "fs": "0.0.1-security",
        "intl": "^1.2.5",
        "path": "^0.12.7",
        "plop": "^2.0.0",
        "react-intl": "^2.4.0",
        "readline": "^1.3.0",
        "save-dev": "^2.0.0",
        "shelljs": "^0.8.2"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/styles -o src/styles",
        "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/styles --watch --recursive",
        "start-js": "react-scripts start",
        "build-js": "react-scripts build",
        "start:dev": "cross-env REACT_APP_ENV=dev npm-run-all -p watch-css start-js",
        "start:uat": "cross-env REACT_APP_ENV=uat npm-run-all -p watch-css start-js",
        "prebuild": "npm-run-all build:clean",
        "build:clean": "rimraf ./build",
        "build:production": "cross-env REACT_APP_ENV=production npm-run-all build-css build-js",
        "build:staging": "cross-env REACT_APP_ENV=production npm-run-all build-css build-js",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject",

        "generate": "plop --plopfile internals/generators/index.js",
        "extract-intl": "babel-node --presets env,stage-0 -- ./internals/scripts/extract-intl.js"
    },
    "babel": {
        "presets": [
            [
                "env",
                {
                    "modules": false
                }
            ],
            "react",
            "stage-0"
        ]
    }
}
